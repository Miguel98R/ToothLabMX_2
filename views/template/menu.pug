.panel.one(style="width:95px;").bg-dark
    .card.bg-dark
        .card-header(style="border-bottom: 5px solid #999999")
            h5.text-white.fw-bold Menu
        .card-body(style="max-height:550px; overflow-y:auto;")
                if menu
                    each val , index in menu
                        if(val.elements)
                            each val2, index2 in val.elements
                                div.p-0(style="width:100%;border-bottom: 3px solid #999")
                                    a(href=val2.ref style="width:100%;")
                                        img.my-3(src=val2.img style="max-height:55px;" title=val2.title)



